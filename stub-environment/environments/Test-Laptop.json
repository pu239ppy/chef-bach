{
  "name": "Test-Laptop",
  "override_attributes": {
    "bcpc": {
      "management": {
        "vip" : "10.0.100.5"
      },
      "floating": {
        "vip" : "192.168.100.5"
      },
      "networks": {
        "subnet1": {
          "management": {
            "vip" : "10.0.100.5",
            "interface" : "eth0",
            "netmask" : "255.255.255.0",
            "cidr" : "10.0.100.0/24",
            "gateway" : "10.0.100.3"
          },
          "storage": {
            "interface" : "eth1",
            "netmask" : "255.255.255.0",
            "cidr" : "172.16.100.0/24",
            "gateway" : "172.16.100.3"
          },
          "floating": {
            "vip" : "192.168.100.5",
            "interface" : "eth2",
            "netmask" : "255.255.255.0",
            "cidr" : "192.168.100.0/24",
            "gateway" : "192.168.100.3",
            "available_subnet" : "192.168.100.128/25"
          }
        }
      },
      "bootstrap": {
        "interface" : "eth0",
        "pxe_interface" : "eth1",
        "hostname" : "bcpc-bootstrap",
        "server" : "10.0.100.3",
        "cluster_def_path": "/cluster-def/",
        "dhcp_subnet" : "10.0.100.0",
        "dhcp_range" : "10.0.100.14 10.0.100.250",
        "preseed": {
          "additional_packages": [ "openssh-server",
                                   "lldpd",
                                   "virtualbox-guest-utils" ]
        }
      },
      "hadoop" : {
        "yarn" : {
          "resourcemanager" : {
            "yarn.client.failover-sleep-base-ms": 30000
          }
        },
        "hdfs" : {
          "dfs_replication_factor" : 1
        },
        "oozie": {
          "memory_opts": "-Xmx512m -XX:MaxPermSize=64m"
        }
      }
    },
    "ntp": {
       "servers" : [ "0.pool.ntp.org",
                     "1.pool.ntp.org",
                     "2.pool.ntp.org",
                     "3.pool.ntp.org" ]
    },
    "chef_client": {
      "server_url": "http://10.0.100.3:4000",
      "cache_path": "/var/chef/cache",
      "config": {
         "verify_api_cert": false,
         "ssl_verify_mode": "verify_none"
      },
      "backup_path": "/var/chef/backup",
      "validation_client_name": "chef-validator",
      "run_path": "/var/chef"
    }
  },
  "json_class": "Chef::Environment",
  "description": "Laptop test environment",
  "cookbook_versions": {
  },
  "chef_type": "environment"
}
